{
  "compilerOptions": {
    "outDir": "./dist/",
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "esnext",
    "target": "es6",
    "lib": [ "es5", "es6", "dom", "es7", "esnext" ],
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": true,
    "noImplicitThis": true
  }
}
